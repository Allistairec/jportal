ENDCODE

# used by Rutter
PROC ForUpd
INPUT
  Id            int
  QueueId       char  (16)
  Status        tinyint
OUTPUT (single)
  MessageID     int
  Streamid      int
  Streamref     char (128)
  Sourcequeue   char (16)
  QueueID       char (16)
  DateCreated   datetime
  Status        TinyInt
  MessageLen    smallint
  MessageData   image(16000)
  MessageType   tinyint
  ResponseQueue char  (16)
SQL CODE
   SELECT
      r.MessageID
     ,r.Streamid
     ,r.Streamref
     ,r.Sourcequeue
     ,r.QueueID
     ,to_char(r.DateCreated, 'YYYYMMDDHH24MISS')
     ,r.Status
     ,r.MessageLen
     ,r.MessageData
     ,r.MessageType
     ,m.ResponseQueue
   FROM
      Response r, message m
   WHERE
          r.ID = :ID
      AND r.Queueid = :Queueid
      AND r.Status = :Status
      AND r.MessageID = m.id
      for update
ENDCODE

