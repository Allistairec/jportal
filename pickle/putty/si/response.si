# Used By PE Front End
PROC ByQueue
INPUT
   InQueue        char(16)
   InStatus       tinyint
   InDateFrom     DateTime
   InDateTo       DateTime
OUTPUT(Multiple)
   Id            int
   MessageID     int
   Streamid      int
   Streamref     char (128)
   Sourcequeue   char (16)
   QueueID       char (16)
   DateCreated   datetime
   Status        TinyInt    (Idle=0 Sent=1 Complete=2 Error=3)
   MessageLen    smallint
   MessageType   tinyint    (XML=0 Text=1)
   USId          char (16)
   Tmstamp       timestamp
SQL CODE
   SELECT
     Id
     ,MessageID
     ,Streamid
     ,Streamref
     ,Sourcequeue
     ,QueueID
     ,to_char(DateCreated, 'YYYYMMDDHH24MISS')
     ,Status
     ,MessageLen
     ,MessageType
     ,USId
     ,to_char(Tmstamp, 'YYYYMMDDHH24MISS')
   FROM
     Response
   WHERE
          QueueId = :InQueue
      AND Status = :InStatus
      AND DateCreated between to_date(:InDateFrom, 'YYYYMMDDHH24MISS')
      AND                     to_date(:InDateTo  , 'YYYYMMDDHH24MISS')
      ORDER BY Id
ENDCODE

