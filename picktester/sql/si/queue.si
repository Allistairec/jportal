DATABASE putty FLAGS ''PREDEFINED'=USId(16)' 'when=TmStamp'
PACKAGE  vlab
OUTPUT   Queue
SERVER   npu
SCHEMA   vlab

TABLE Queue
    OPTIONS "descr=Queues" "show=Name"
  Id           Char(16)
  Name         Char(64)
  Descr        Char(256)
  InputDriver  Char(16)
  OutputDriver Char(16)
  QueueType    Int      (Message=0, Stream=1, Response=2, Reply=3)
  Priority     byte
  Status       byte  (Active=0 Inactive=1 MarkForDelete=2)
  USId         Char(16)
  TmStamp      DateTime

KEY QueueKey PRIMARY
  Id

KEY KEY01
// //TABLESPACE NPUT001M1 INITIAL 40K NEXT 1M
  Name

LINK QueueType QueueType
LINK Drivers   InputDriver
LINK Drivers   OutputDriver

PROC Insert
PROC Update
PROC SelectOne
PROC DeleteOne
PROC Exists
PROC SelectAll
PROC Count

