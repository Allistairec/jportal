// used by SaffyRcv,TibosSnd,TcpIn, TcpOut
PROC Get
INPUT
  QueueName       Char(64)
OUTPUT (Single)
  Id              Char(16)
  Descr           Char(256)
  InputDriver     Char(16)
  OutputDriver    Char(16)
  QueueType       Int // (Message=0, Stream=1, Response=2, Reply=3)
  Priority        byte
  Status          byte
sqlcode
  SELECT
     Id
    ,Descr
    ,InputDriver
    ,OutputDriver
    ,QueueType
    ,Priority
    ,Status
  FROM
     Queue
  Where
     Name = :QueueName
endcode

