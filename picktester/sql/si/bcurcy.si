DATABASE putty FLAGS 'user=USId(16)' 'when=TmStamp'
PACKAGE  vlab
OUTPUT   BCurcy
SERVER   npu
SCHEMA   vlab

TABLE BCurcy

PROC Curcy
OUTPUT 
  Id Char(4)
  Descr Char(64)
  Comments Char(256)
  Status byte
sqlcode
  SELECT Id
       , Descr
       , Comments
       , Status
  FROM Currency
  ORDER BY Id
endcode
