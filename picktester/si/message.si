// Used by nedcor-adapter-ref-in(narin)
PROC MsgIdByRefSourceQ
INPUT
   InReference    char(64)
   InSourceSystem char(16)
   DayRange       int
OUTPUT(Multiple)
   MsgId          int
SQL CODE
   SELECT
     Id
   FROM
     Message
   WHERE
         Reference = :InReference
     AND SourceSysid = :InSourceSystem
     AND DateCreated > sysdate -:DayRange
     ORDER BY Id
ENDCODE

