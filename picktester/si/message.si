# used by Rutter
PROC Queued $PYTHON
INPUT
  Queue         char  (16)
  Status        tinyint
  BackDays      int
OUTPUT (Multiple)
  Id            Int
SQL CODE
  SELECT
     Id
  FROM
     $TableName(64)
  Where
     Queueid = :Queue
     and Status = :Status
     and DateCreated > (sysdate - :BackDays)  and DateCreated <= sysdate
  ORDER BY Id
ENDCODE

# used by Rutter,tibossnd
PROC UpdStatus
INPUT
  Id            int
  StreamCount   tinyint
  Status        tinyint
  USId          char  (16)
SQL CODE
  Update
    Message
  Set
      Status = :Status
     ,StreamCount = StreamCount + :StreamCount
     ,USId = :USId
     ,TMStamp = SYSDATE
  where
     id = :Id
ENDCODE

